<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<opml version="1.0">
    <head>
        <title>GitHub Releases</title>
        <dateCreated>{{ dateCreated }}</dateCreated>
    </head>
    <body>{% for repository in repositories %}{% if repository and repository.githubPath %}
		<outline htmlUrl="{{ repository.githubUrl }}" xmlUrl="{{ repository.releaseRssFeedUrl }}" type="application/atom+xml" description="" text="{{ repository.name }} GitHub Releases"/>{% endif %}{% endfor %}
    </body>
</opml>
